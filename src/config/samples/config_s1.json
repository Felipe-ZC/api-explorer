[
	{
  "title": "Get posts",
  "url": "https://jsonplaceholder.typicode.com/posts",
	"method": "GET"
	},
	{
  "title": "Get users",
  "url": "https://jsonplaceholder.typicode.com/users",
  "method": "GET"
	},
	{
  "title": "Create new post",
  "url": "https://jsonplaceholder.typicode.com/users",
  "method": "POST",
	"body": [	
			{
				"name": "title",
				"type": "text",
				"placeholder": "My post title",
				"required": true
			},
			{
				"name": "body",
				"type": "text",
				"placeholder": "My post body",
				"required": true
			},
			{
				"name": "user Id",
				"type": "number",
				"placeholder": "700",
				"required": true
			}
		]
	},
	{
		"title": "Create new user",
		"url": "https://jsonplaceholder.typicode.com/users",
		"method": "POST",
		"body": [
			{
				"name": "email",
				"type": "email",
				"max": 24,
				"min": 3
			},
			{
				"name": "full-name",
				"type": "text",
				"placeholder": "John Doe",
				"required": true
			},
			{
				"name": "phone",
				"type": "tel",
				"pattern": "[0-9]{3}-[0-9]{3}-[0-9]{4}"
			}
		]
	}
]
