{
  "title": "Add new user",
  "url": "https://jsonplaceholder.typicode.com/users",
  "method": "POST",
  "body": [
    {
      "name": "email",
      "type": "email",
      "max": 24,
      "min": 3,
			"value" : "guy@guysmail.com"
    },
    {
      "name": "full-name",
      "type": "text",
      "placeholder": "John Doe",
      "required": true,
			"value" : "Guy Guyson"
    },
    {
      "name": "phone",
      "type": "tel",
      "pattern": "[0-9]{3}-[0-9]{3}-[0-9]{4}",
			"value" : "305-666-6969"
    }
  ]
}
